{
  "name": "langgraph-visualizer",
  "displayName": "LangGraph Visualizers",
  "description": "Interactive visualization of LangGraph graphs with drag-and-drop nodes, search, and jump-to-code",
  "version": "0.2.0",
  "publisher": "smazee",
  "author": {
    "name": "Naveen Kumar"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Naveenkumarar/langgraph-visualizer"
  },
  "bugs": {
    "url": "https://github.com/Naveenkumarar/langgraph-visualizer/issues"
  },
  "homepage": "https://github.com/Naveenkumarar/langgraph-visualizer#readme",
  "icon": "media/icons/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization",
    "Programming Languages"
  ],
  "keywords": [
    "langgraph",
    "graph",
    "visualization",
    "python",
    "langchain"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "langgraph-visualizer.showGraph",
        "title": "Show LangGraph Visualization",
        "icon": "$(graph)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "langgraph-visualizer.showGraph",
          "when": "editorLangId == python",
          "group": "navigation@1"
        }
      ],
      "explorer/context": [
        {
          "command": "langgraph-visualizer.showGraph",
          "when": "resourceLangId == python",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.x",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "typescript": "^5.1.6"
  }
}