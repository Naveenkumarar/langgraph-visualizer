{
  "name": "langgraph-visualizer",
  "displayName": "LangGraph Visualizers",
  "description": "Interactive visualization of LangGraph graphs with drag-and-drop nodes, search, jump-to-code, and runtime debugging",
  "version": "0.3.1",
  "publisher": "smazee",
  "author": {
    "name": "Naveen Kumar"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Naveenkumarar/langgraph-visualizer"
  },
  "bugs": {
    "url": "https://github.com/Naveenkumarar/langgraph-visualizer/issues"
  },
  "homepage": "https://github.com/Naveenkumarar/langgraph-visualizer#readme",
  "icon": "media/icons/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization",
    "Programming Languages",
    "Debuggers"
  ],
  "keywords": [
    "langgraph",
    "graph",
    "visualization",
    "python",
    "langchain",
    "debugging",
    "runtime"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "langgraph-visualizer.showGraph",
        "title": "Show LangGraph Visualization",
        "icon": "$(graph)"
      },
      {
        "command": "langgraph-visualizer.startDebug",
        "title": "LangGraph: Start Debug Session",
        "icon": "$(debug-start)"
      },
      {
        "command": "langgraph-visualizer.stopDebug",
        "title": "LangGraph: Stop Debug Session",
        "icon": "$(debug-stop)"
      },
      {
        "command": "langgraph-visualizer.toggleDebug",
        "title": "LangGraph: Toggle Debug Session",
        "icon": "$(debug-alt)"
      },
      {
        "command": "langgraph-visualizer.pauseDebug",
        "title": "LangGraph: Pause Execution",
        "icon": "$(debug-pause)"
      },
      {
        "command": "langgraph-visualizer.resumeDebug",
        "title": "LangGraph: Resume Execution",
        "icon": "$(debug-continue)"
      },
      {
        "command": "langgraph-visualizer.stepDebug",
        "title": "LangGraph: Step to Next Node",
        "icon": "$(debug-step-over)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "langgraph-visualizer.showGraph",
          "when": "editorLangId == python",
          "group": "navigation@1"
        },
        {
          "command": "langgraph-visualizer.startDebug",
          "when": "editorLangId == python",
          "group": "navigation@2"
        }
      ],
      "explorer/context": [
        {
          "command": "langgraph-visualizer.showGraph",
          "when": "resourceLangId == python",
          "group": "navigation@1"
        }
      ]
    },
    "keybindings": [
      {
        "command": "langgraph-visualizer.pauseDebug",
        "key": "f6",
        "when": "editorLangId == python"
      },
      {
        "command": "langgraph-visualizer.resumeDebug",
        "key": "f5",
        "when": "editorLangId == python"
      },
      {
        "command": "langgraph-visualizer.stepDebug",
        "key": "f10",
        "when": "editorLangId == python"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "generate-icons": "node scripts/generate-icons.js",
    "package": "vsce package"
  },
  "dependencies": {
    "ws": "^8.14.0"
  },
  "bundledDependencies": [
    "ws"
  ],
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.x",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "typescript": "^5.1.6"
  }
}
